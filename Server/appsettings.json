{
  "ConnectionStrings": {
    "DefaultConnection": "Server=${DB_HOST};Port=${DB_PORT};User ID=${DB_USER};Password=${DB_PASSWORD};Database=${DB_NAME};SslMode=REQUIRED"
  },
  "MongoDBSettings": {
    "ConnectionString": "mongodb+srv://${MONGO_USER}:${MONGO_PASSWORD}@${MONGO_HOST}/?retryWrites=true&w=majority&appName=Cluster0",
    "DatabaseName": "${MONGO_NAME}"
  },
  "AWS": {
    "AccessKey": "${AWS_ACCESS_KEY}",
    "SecretKey": "${AWS_SECRET_KEY}",
    "Region": "${AWS_REGION}",
    "BucketName": "${AWS_BUCKET}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSection": {
    "Key": "TthlUMLddbON00TthlUMLddbON00TthlUMLddbON00",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}"
  }
}
